<p id="notice"><%= notice %></p>

<h1>Admin Commercials</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_commercials.each do |admin_commercial| %>
      <tr>
        <td><%= link_to 'Show', admin_commercial %></td>
        <td><%= link_to 'Edit', edit_admin_commercial_path(admin_commercial) %></td>
        <td><%= link_to 'Destroy', admin_commercial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Commercial', new_admin_commercial_path %>
